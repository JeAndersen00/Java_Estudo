import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int cod, qtd;
		double preco, valorFinal;
		
		cod = sc.nextInt();
		
		if (cod == 1) {
			preco = 4.00;
		}
		else if (cod == 2) {
			preco = 4.50;
		}
		else if (cod == 3) {
			preco = 5.00;
		}
		else if (cod == 4) {
			preco = 2.00;
		}
		else {
			preco = 1.50;
		}
		
		qtd = sc.nextInt();
		
		valorFinal = preco * qtd;
		
		System.out.printf("Total: R$ %.2f%n", valorFinal);
		
		sc.close();
	}

}
