import java.util.Locale;
import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int x;
		
		x = sc.nextInt();
		
		String colocacao;
		
		switch (x) {
		case 1:
			colocacao = "Primeiro";
			break;
		case 2:
			colocacao = "segundo";
			break;
		default:
			colocacao = "me deu pregui√ßa";
			break;
		}
		
		
		
		
		
		sc.close();
	}
}
